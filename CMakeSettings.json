	{
	  "name": "OUT-Native-Debug-Clang",
	  "generator": "Ninja",
	  "configurationType": "Debug",
	  "buildRoot": "${projectDir}\\${name}\\build",
	  "installRoot": "${projectDir}\\${name}\\install",
	  "cmakeCommandArgs": "-G \"Ninja\" -DCMAKE_C_COMPILER=clang-cl -DCMAKE_CXX_COMPILER=clang-cl",
	  "buildCommandArgs": "",
	  "ctestCommandArgs": "",
	  "inheritEnvironments": [ "clang_cl_x64_x64" ],
	  "variables": []
	}
  ]
